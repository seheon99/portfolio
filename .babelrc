{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "./app",
            "rootPathPrefix": "@/"
          },
          {
            "rootPathSuffix": "./components",
            "rootPathPrefix": "#/"
          }
        ]
      }
    ]
  ]
}